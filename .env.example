# OpenAI - REQUIRED for real model runs
OPENAI_API_KEY=your-openai-key-here

# LangSmith - REQUIRED if you want tracing/telemetry
LANGSMITH_API_KEY=your-langsmith-api-key-here
LANGSMITH_TRACING=true
LANGSMITH_ENDPOINT=https://api.smith.langchain.com
LANGSMITH_PROJECT=longevity-hackathon

# Optional: Valyu
# API key for Valyu search (https://platform.valyu.network/)
VALYU_API_KEY=your-valyu-api-key-here
# Validation URL for local Valyu claim checking (already has a default in code)
# VALYU_URL=http://localhost:3000/validate

# ElevenLabs Agents (optional; for live voice UI)
# Public Agent ID from https://elevenlabs.io/app/agents (ensure agent is public)
# VITE_ prefix exposes it to the Vite frontend build
VITE_ELEVEN_AGENT_ID=

# Duo agents (optional defaults)
VITE_ELEVEN_LEO_AGENT_ID=
VITE_ELEVEN_LUNA_AGENT_ID=

# Optional: API base for frontend â†’ backend calls
# Example: http://localhost:5174
VITE_API_BASE=

# Optional default voices for Duo (Text + TTS)
# Set distinct ElevenLabs voice IDs for LEO and LUNA
VITE_ELEVEN_LEO_VOICE_ID=
VITE_ELEVEN_LUNA_VOICE_ID=
